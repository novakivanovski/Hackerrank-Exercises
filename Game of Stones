#include <cmath>
#include <cstdio>
#include <vector>
#include <iostream>
#include <algorithm>
using namespace std;


int main() {
    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   
    int n = 0;
    cin >> n;
    int stones;
    for (int i = 0; i < n; i ++)
    {
        cin >> stones; 
        if (stones % 7 == 0 || stones % 7 == 1)
            cout << "Second" << endl;
        else
        cout << "First" << endl;
    }
    return 0;
}
